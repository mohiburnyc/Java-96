<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="audi" class="co.edureka.spring.core.beans.Car">
  	<property name="model" value="Audi"/>
  	<property name="cost" value="6350000"/>
  </bean>

  <bean id="susheel" class="co.edureka.spring.core.beans.Customer">
  	<constructor-arg value="Susheel Kumar"/>
  	<constructor-arg ref="audi"/>
  </bean>
</beans>
